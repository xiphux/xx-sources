 25-akpm/arch/ia64/configs/sn2_defconfig |    1 +
 1 files changed, 1 insertion(+)

diff -puN arch/ia64/configs/sn2_defconfig~cpusets-make-config_cpusets-the-default-in-sn2_defconfig arch/ia64/configs/sn2_defconfig
--- 25/arch/ia64/configs/sn2_defconfig~cpusets-make-config_cpusets-the-default-in-sn2_defconfig	2004-09-21 22:15:13.066602696 -0700
+++ 25-akpm/arch/ia64/configs/sn2_defconfig	2004-09-21 22:15:13.071601936 -0700
@@ -29,6 +29,7 @@ CONFIG_KALLSYMS_ALL=y
 # CONFIG_KALLSYMS_EXTRA_PASS is not set
 CONFIG_FUTEX=y
